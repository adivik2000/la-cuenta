<?xml version="1.0" encoding="UTF-8"?>
<manifest android:installLocation="auto" package="org.blanco.lacuenta" xmlns:android="http://schemas.android.com/apk/res/android" android:versionName="1.1" android:versionCode="2">
    <application android:allowClearUserData="true"
        android:debuggable="false" android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:label="@string/app_name" android:name=".MainActivity" android:windowSoftInputMode="stateAlwaysVisible|adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".SettingsActivity"/>
        <provider
            android:authorities="org.blanco.lacuenta.db.splitscontentprovider" android:name="org.blanco.lacuenta.db.SPLITSContentProvider"/>
    </application>
    <uses-sdk android:maxSdkVersion="9" android:minSdkVersion="8" android:targetSdkVersion="8"/>
</manifest>
